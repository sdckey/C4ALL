class HashController {

  SearchClass Search;

  HashController()
  {
    Search = new SearchClass();
    Search.Create();
  }

  String GetMd5Hash(FileClass f, long length){
    String hash = Search.ComputeHash(f, length);

    return hash;
  }

  String GetSha1Hash(FileClass f, long length)
  {
    DigestClass digest();
    return digest.Compute(f, SHA1, length);
  }
}
